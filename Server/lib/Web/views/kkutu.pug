extends layout

mixin Dialog(id, w, h, t, nocls)
	div.dialog(id=id, style=`width: ${w}px; height: ${h}px;`)
		div(class=nocls ? 'no-close dialog-head' : 'dialog-head')
			div.dialog-title(style=`width: ${w - 20}px;`)!= t || ''
		div.dialog-body(style='font-size: 13px;')
			block
	
mixin Kialog(id, w, h, t, nocls)
	div.dialog(id=id, style=`width: ${w}px; height: ${h}px;`)
		div(class=nocls ? 'no-close dialog-head' : 'dialog-head')
			div.dialog-title(style=`width: ${w - 72}px;`)!= t || ''
		div.dialog-body(style='font-size: 13px;')
			block
	
mixin Uialog(id, w, h, t)
	div.uialog(id=id, style=`width: ${w}px; height: ${h}px;`)
		div(class='uialog-head')
			div.uialog-title(style=`width: ${w - 20}px;`)!= t || ''
		div.uialog-body(style='font-size: 13px;')
			block

mixin GameOption(key, prefix)
	- var name = locals.OPTIONS[key].name;
	- var sid = name.toLowerCase();
	div.dialog-opt(id=`${prefix}-${sid}-panel`)
		input.game-option(id=`${prefix}-${sid}`, type='checkbox', style='margin-top: 5px; width: auto;')
		label(for=`${prefix}-${sid}`)= L(`opt${name}`)
		+Expl(true)
			div!= L(`expl${name}`)

mixin SettingOption(id, text, st)
	div.dialog-opt(style=st || "")
		input(id=id, type='checkbox', style='margin-top: 5px; width: auto;')
		label(for=id)= text

mixin SliderBar(id, wd)
	div.dialog-opt(style="")
		input(id=id, type='range', min='0', max='100', value='100' style=`width: ${wd}px;`)

block Subject
	title= L('title')

block JS
	script(type='text/javascript').
		function pfA(){
			FingerprintJS.load().then(pfB => {
				pfB.get().then(result => {
					var sInv;
					if(window._setInterval) sInv = window._setInterval;
					else sInv = setInterval;
					var sendFingerprint = sInv(()=>{
						if(window.ws && window.ws.readyState){
							ws.send(JSON.stringify({type:'fingerprint', data:result.visitorId+'/'+result.components.canvas.value.data.slice(22)}));
							clearInterval(sendFingerprint);
						}
					}, 500);
				});
			});
		}
	script(type='text/javascript', async src='//cdn.jsdelivr.net/npm/@fingerprintjs/fingerprintjs@3/dist/fp.min.js' onload='pfA()')
	script(type='text/javascript', src='/js/in_game_kkutu.min.js')
	script(type='text/javascript', src='/js/chanIO.min.js')
	script(type='text/javascript', src='https://cdn.jsdelivr.net/gh/pink-flower/pink-kkutu@latest/js/fabric.min.js')
	script(src='https://www.google.com/recaptcha/api.js')

